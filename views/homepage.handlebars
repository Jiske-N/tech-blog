 {{#each blogPosts as |blogPost|}}
      <div class="container bg-dark">
<div class="card mt-4">
  <h5 class="card-header bg-success text-white">{{blogPost.title}}</h5>
  <div class="card-body">
    <p class="card-text">{{blogPost.content}}</p>
    <blockquote class="blockquote mb-0">
      <footer class="blockquote-footer">{{blogPost.user.name}}  <cite title="Date">{{blogPost.updatedAt}}</cite></footer>
    </blockquote>
  </div>
</div>
</div>
{{/each}}
